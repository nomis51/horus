@inherits MetadataListBase

<MudStack Spacing="2"
          Style="margin-top: .5rem">
    @foreach (var metadata in Metadatas)
    {
        <MetadataField Key="@metadata.Key"
                       Value="@metadata.Value"
                       Type="@metadata.Type"
                       OnValueChanged="@(e => UpdateMetadata(e.Item1, e.Item2, metadata))"/>
    }
</MudStack>